{
    "name": "TwitterSpacesWiretapExtension",
    "description": "TwitterSpacesWiretapExtension",
    "version": "1.0.2",
    "manifest_version": 3,
    "action": {
        "default_popup": "html/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*"
            ],
            "js": [
                "js/TwitterSpacesWiretapExtension.js",
                "js/hls.js/hls.min.js"
            ]
        }
    ]
}