{
    "name": "TwitterSpacesWiretapExtension",
    "description": "TwitterSpacesWiretapExtension",
    "version": "1.0.4",
    "manifest_version": 3,
    "action": {
        "default_popup": "html/popup.html"
    },
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*"
            ],
            "js": [
                "js/TwitterSpacesWiretapExtension.js",
                "js/hls.js/hls.min.js"
            ]
        }
    ]
}